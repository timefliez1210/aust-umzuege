{
	"name": "alex-aust",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lighthouse": "npm run build && npm run preview & sleep 3 && lhci autorun --config=lighthouserc.js && kill %1",
		"lighthouse:mobile": "npm run build && npm run preview & sleep 3 && lhci autorun --config=lighthouserc.js --preset=mobile && kill %1",
		"lighthouse:open": "open lighthouse-reports/index.html || xdg-open lighthouse-reports/index.html"
	},
	"devDependencies": {
		"@lhci/cli": "^0.15.1",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"lighthouse": "^13.0.3",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"typescript": "^5.9.3",
		"vite": "^7.2.6",
		"xlsx": "^0.18.5"
	},
	"dependencies": {
		"lucide-svelte": "^0.562.0",
		"sharp": "^0.34.5"
	}
}
